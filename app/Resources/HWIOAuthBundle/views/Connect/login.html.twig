{% extends '@HWIOAuth/layout.html.twig' %}
{% block hwi_oauth_content %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    {% for owner in hwi_oauth_resource_owners() %}
        <li class="login">
        {% if block(owner) is defined %}
            {{ block(owner) }}
        {% else %}
            <a href="{{ hwi_oauth_login_url(owner) }}">{{ owner | trans({}, 'HWIOAuthBundle') }}</a> <br />
        {% endif %}
        </li>
    {% endfor %}
{% endblock hwi_oauth_content %}



{% block vkontakte %}
<a class="vkontakte image" href="{{ hwi_oauth_login_url(owner) }}" title="{{ 'auth.vk'|trans }}">

    </a>
{% endblock %}

{% block facebook %}
    <a href="{{ hwi_oauth_login_url(owner) }}" title="{{ 'auth.facebook'|trans }}">
        <img src="" />
    </a>
{% endblock %}

